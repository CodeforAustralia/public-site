<div class="pull-right people-nav">
  <div class="menu-links">
    <%= link_to 'Blog', "http://blog.codeforaustralia.org", class: 'btn' %>
    <%= link_to 'Guides', "http://guides.codeforaustralia.org", class: 'btn' %>
  </div>
  <% if signed_in? %>
    <%= link_to 'Volunteer Profiles', profiles_path, class: 'btn' %>
    <%= link_to 'Edit Profile', edit_profile_path, class: 'btn btn-primary' %>
    <%= link_to 'Sign Out', destroy_person_session_path, method: :delete, class: 'btn btn-danger' %>
  <% else %>
    <%= link_to 'Sign In', new_person_session_path, class: 'btn btn-primary' %>
  <% end %>
</div>
